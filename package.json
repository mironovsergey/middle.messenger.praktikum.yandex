{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "scripts": {
    "copy-static": "cp -vR ./static ./build",
    "dev": "npm run copy-static && parcel src/index.html --dist-dir build --no-cache --port 3000",
    "build": "npm run copy-static && parcel build src/index.html --dist-dir build --no-cache",
    "start": "node server.js"
  },
  "targets": {
    "default": {
      "sourceMap": false,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.8.2",
    "@parcel/transformer-sass": "^2.8.2",
    "parcel": "^2.8.2",
    "parcel-transformer-hbs": "^1.0.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "handlebars": "^4.7.7"
  }
}
